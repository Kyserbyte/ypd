{
    "name": "kyserbyte/ypd",
    "description": "Ypd Php Decorator implements \"decorators/annotations and meta-programming\" in PHP, via doc comments.",
    "type": "library",
    "keywords": [
        "php",
        "decorator",
        "annotation",
        "meta-programming"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Augusto \"Kyserbyte\" Pace",
            "email": "kyserbyte@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/Kyserbyte/ypd/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "YPD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YPD\\Demo\\": "demo/",
            "YPD\\Tests\\": "tests/"
        }
    }
}